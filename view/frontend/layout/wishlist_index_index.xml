<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="customer.wishlist">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Tigren_CompanyAccount::wishlist/view.phtml</argument>
            </action>
        </referenceBlock>

        <move element="customer.wishlist.item.price" destination="customer.wishlist.item.inner" after="customer.wishlist.item.comment"/>

    </body>
</page>
